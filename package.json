    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.3.1",
    "rn-nodeify": "github:tradle/rn-nodeify",
    "ts-jest": "^22.4.6",
    "tslib": "^1.9.1",
    "typescript": "^2.8.3"
